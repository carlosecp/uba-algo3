Object subclass: #Pelicula	instanceVariableNames: 'nombre duracion anio puntajes'	classVariableNames: ''	package: 'FIUBA-TP0'!!Pelicula methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/13/2022 20:24'!establecerAnio: unAnio	anio := unAnio! !!Pelicula methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/13/2022 20:23'!establecerNombre: unNombre	nombre := unNombre! !!Pelicula methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/13/2022 20:32'!calificarCon: unPuntaje	puntajes add: unPuntaje! !!Pelicula methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/13/2022 20:33'!puntajePromedio	^ puntajes average ! !!Pelicula methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/13/2022 20:23'!establecerDuracionEnMinutos: unaDuracion	duracion := unaDuracion! !!Pelicula methodsFor: 'initialization' stamp: 'CarlosCastillo 4/13/2022 20:32'!initialize 	puntajes := OrderedCollection new! !!Pelicula methodsFor: 'accessing' stamp: 'CarlosCastillo 4/13/2022 20:35'!anio	^ anio! !!Pelicula methodsFor: 'accessing' stamp: 'CarlosCastillo 4/13/2022 20:26'!duracion	^ duracion! !!Pelicula methodsFor: 'accessing' stamp: 'CarlosCastillo 4/13/2022 20:26'!nombre	^ nombre! !Object subclass: #PelisRankings	instanceVariableNames: 'peliculas'	classVariableNames: ''	package: 'FIUBA-TP0'!!PelisRankings methodsFor: 'initialization' stamp: 'CarlosCastillo 4/13/2022 20:25'!initialize 	peliculas := OrderedCollection new! !!PelisRankings methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/13/2022 20:35'!mejorPeliculaDeTodosLosTiempos	^ (peliculas detectMax: [ :peli | peli puntajePromedio ]) nombre! !!PelisRankings methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/13/2022 20:26'!peliculaMasLarga	^ (peliculas detectMax: [ :peli | peli duracion ]) nombre! !!PelisRankings methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/13/2022 20:25'!agregarPelicula: unaPelicula	peliculas add: unaPelicula! !!PelisRankings methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/13/2022 20:36'!mejorPeliculaDelAnio: unAnio	^ ((peliculas select: [ :peli | peli anio = unAnio ])		detectMax: [ :peli | peli puntajePromedio ]) nombre! !!PelisRankings methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/13/2022 20:29'!calificarPelicula: unNombrePelicula conPuntaje: unPuntaje	^ (peliculas detect: [ :peli | peli nombre = unNombrePelicula ]) calificarCon: unPuntaje! !!PelisRankings methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/13/2022 20:31'!obtenerPuntajePromedioDe: unNombrePelicula	^ (peliculas detect: [ :peli | peli nombre = unNombrePelicula ]) puntajePromedio! !
