Object subclass: #Llamada	instanceVariableNames: 'duracion horario tarifaPorMinuto'	classVariableNames: ''	package: 'TP1'!!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 15:34'!duracion: unaDuracion	duracion := unaDuracion! !!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:27'!descripcion	self subclassResponsibility ! !!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:28'!horario 	^ horario! !!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 17:12'!esLlamadaDeLargaDuracion	^ duracion > 30	! !!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:29'!duracion	^ duracion! !!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:40'!horario: unHorario	horario := unHorario! !!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 17:14'!costoSegunDuracion	| total porcentajeDescuento |	total := duracion * tarifaPorMinuto.	porcentajeDescuento := self esLlamadaDeLargaDuracion ifTrue: [ 0.10 ] ifFalse: [ 0 ].	^ total - (total * porcentajeDescuento)! !!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:10'!tarifaPorMinuto: unaTarifa	tarifaPorMinuto := unaTarifa! !!Llamada methodsFor: 'initialization' stamp: 'CarlosCastillo 4/17/2022 17:17'!initialize 	duracion := 0.	horario := 0.	tarifaPorMinuto := 0! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Llamada class	instanceVariableNames: ''!!Llamada class methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:09'!conDuracion: unaDuracion ConTarifa: unaTarifa	^ (self new) duracion: unaDuracion; tarifaPorMinuto: unaTarifa	! !Llamada subclass: #LlamadaInternacional	instanceVariableNames: 'destino'	classVariableNames: ''	package: 'TP1'!!LlamadaInternacional methodsFor: 'initialization' stamp: 'CarlosCastillo 4/17/2022 17:18'!initialize 	super initialize.	destino := ''! !!LlamadaInternacional methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:43'!descripcion 	^ 'La llamada se hizo a ', destino, ' a las ', horario asString, ' hs durando ', duracion asString, ' minutos'! !!LlamadaInternacional methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:28'!destino	^ destino! !!LlamadaInternacional methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:23'!destino: unDestino	destino := unDestino! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!LlamadaInternacional class	instanceVariableNames: ''!!LlamadaInternacional class methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:22'!conDuracion: unaDuracion ConTarifa: unaTarifa ConDestino: unDestino	^ (self new) duracion: unaDuracion; tarifaPorMinuto: unaTarifa; destino: unDestino	! !!LlamadaInternacional class methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:40'!conDuracion: unaDuracion ConHorario: unHorario ConTarifa: unaTarifa ConDestino: unDestino	^ (self new) duracion: unaDuracion; horario: unHorario; tarifaPorMinuto: unaTarifa; destino: unDestino	! !Llamada subclass: #LlamadaNacional	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!LlamadaNacional methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:43'!descripcion 	^ 'La llamada se hizo a las ', horario asString, ' hs durando ', duracion asString, ' minutos'! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!LlamadaNacional class	instanceVariableNames: ''!!LlamadaNacional class methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:39'!conDuracion: unaDuracion ConHorario: unHorario ConTarifa: unaTarifa	^ (self new) duracion: unaDuracion; horario: unHorario; tarifaPorMinuto: unaTarifa! !Object subclass: #TeleAlgo	instanceVariableNames: 'abonoMensual llamadas horariosHabiles tarifaInternacional'	classVariableNames: ''	package: 'TP1'!!TeleAlgo methodsFor: 'initialization' stamp: 'CarlosCastillo 4/17/2022 15:55'!initialize	abonoMensual := 0.	llamadas := OrderedCollection new.	horariosHabiles := Interval from: 8 to: 20.	tarifaInternacional := 0! !!TeleAlgo methodsFor: 'private' stamp: 'CarlosCastillo 4/17/2022 17:19'!horarioEstaEnHorarioHabil: unHorario	^ (horariosHabiles includes: unHorario)! !!TeleAlgo methodsFor: 'private' stamp: 'CarlosCastillo 4/17/2022 17:23'!calcularTarifaPorMinutoSegunHorario: unHorario	^ (self horarioEstaEnHorarioHabil: unHorario) ifTrue: [ 0.2 ] ifFalse: [ 0.1 ]! !!TeleAlgo methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 15:42'!aplicarTarifaPorMinutoALlamadaInternacional: unaTarifa	tarifaInternacional := unaTarifa! !!TeleAlgo methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:41'!registrarLlamadaInternacionalCon: unaDuracion HechaEnElHorario: unHorario Hacia: unDestino	llamadas add: (LlamadaInternacional		conDuracion: unaDuracion		ConHorario: unHorario		ConTarifa: tarifaInternacional		ConDestino: unDestino)! !!TeleAlgo methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 17:02'!consultarPorLlamadaEfectuadaMasCostosa	| llamadaMasCostosa |	llamadaMasCostosa := llamadas detectMax: [ :llamada | llamada costoSegunDuracion ].	^ 'La llamada mas costosa: ', llamadaMasCostosa descripcion! !!TeleAlgo methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 17:22'!registrarLlamadaNacionalConDuracion: unaDuracion HechaEnElHorario: unHorario	| tarifa |		tarifa := self calcularTarifaPorMinutoSegunHorario: unHorario.	llamadas add: (LlamadaNacional		conDuracion: unaDuracion		ConHorario: unHorario		ConTarifa: tarifa)! !!TeleAlgo methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 13:50'!abonoMensual: unAbonoMensual	abonoMensual := unAbonoMensual ! !!TeleAlgo methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 17:05'!calcularGastoFinal	^ abonoMensual + (llamadas sum: [ :llamada | llamada costoSegunDuracion ]).! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!TeleAlgo class	instanceVariableNames: ''!!TeleAlgo class methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 13:51'!conAbonoMensual: unAbonoMensual	^ (self new) abonoMensual: unAbonoMensual! !