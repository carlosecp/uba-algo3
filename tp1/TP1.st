Object subclass: #Llamada	instanceVariableNames: 'duracion horario tarifa'	classVariableNames: ''	package: 'TP1'!!Llamada methodsFor: 'initialization' stamp: 'CarlosCastillo 4/18/2022 20:10'!initialize 	duracion := 0.	horario := 0.	tarifa := 0! !!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:27'!descripcion	self subclassResponsibility ! !!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/18/2022 20:10'!tarifa: unaTarifa	tarifa := unaTarifa! !!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 15:34'!duracion: unaDuracion	duracion := unaDuracion! !!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:28'!horario 	^ horario! !!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/18/2022 09:10'!calcularDescuento	^ self calcularTotalSinDescuento * self calcularPorcentajeDescuento ! !!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/18/2022 08:55'!calcularPorcentajeDescuento 	^ self esLlamadaDeLargaDuracion ifTrue: [ 0.10 ] ifFalse: [ 0 ]! !!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:29'!duracion	^ duracion! !!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 17:12'!esLlamadaDeLargaDuracion	^ duracion > 30	! !!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/18/2022 09:10'!calcularTotalConDescuento	^ self calcularTotalSinDescuento - self calcularDescuento ! !!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/18/2022 20:10'!calcularTotalSinDescuento	^ duracion * tarifa! !!Llamada methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:40'!horario: unHorario	horario := unHorario! !Llamada subclass: #LlamadaInternacional	instanceVariableNames: 'destino'	classVariableNames: ''	package: 'TP1'!!LlamadaInternacional methodsFor: 'initialization' stamp: 'CarlosCastillo 4/17/2022 17:18'!initialize 	super initialize.	destino := ''! !!LlamadaInternacional methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:43'!descripcion 	^ 'La llamada se hizo a ', destino, ' a las ', horario asString, ' hs durando ', duracion asString, ' minutos'! !!LlamadaInternacional methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:28'!destino	^ destino! !!LlamadaInternacional methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:23'!destino: unDestino	destino := unDestino! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!LlamadaInternacional class	instanceVariableNames: ''!!LlamadaInternacional class methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/18/2022 20:14'!conDuracion: unaDuracion conHorario: unHorario conTarifa: unaTarifa conDestino: unDestino	^ (self new) duracion: unaDuracion; horario: unHorario; tarifa: unaTarifa; destino: unDestino	! !Llamada subclass: #LlamadaNacional	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!LlamadaNacional methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 16:43'!descripcion 	^ 'La llamada se hizo a las ', horario asString, ' hs durando ', duracion asString, ' minutos'! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!LlamadaNacional class	instanceVariableNames: ''!!LlamadaNacional class methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/18/2022 20:13'!conDuracion: unaDuracion conHorario: unHorario conTarifa: unaTarifa	^ (self new) duracion: unaDuracion; horario: unHorario; tarifa: unaTarifa! !Object subclass: #TeleAlgo	instanceVariableNames: 'abonoMensual llamadas horariosHabiles tarifaInternacional'	classVariableNames: ''	package: 'TP1'!!TeleAlgo methodsFor: 'initialization' stamp: 'CarlosCastillo 4/17/2022 15:55'!initialize	abonoMensual := 0.	llamadas := OrderedCollection new.	horariosHabiles := Interval from: 8 to: 20.	tarifaInternacional := 0! !!TeleAlgo methodsFor: 'private' stamp: 'CarlosCastillo 4/17/2022 17:19'!horarioEstaEnHorarioHabil: unHorario	^ (horariosHabiles includes: unHorario)! !!TeleAlgo methodsFor: 'private' stamp: 'CarlosCastillo 4/17/2022 17:23'!calcularTarifaPorMinutoSegunHorario: unHorario	^ (self horarioEstaEnHorarioHabil: unHorario) ifTrue: [ 0.2 ] ifFalse: [ 0.1 ]! !!TeleAlgo methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 15:42'!aplicarTarifaPorMinutoALlamadaInternacional: unaTarifa	tarifaInternacional := unaTarifa! !!TeleAlgo methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/18/2022 20:05'!registrarLlamadaInternacionalCon: unaDuracion HechaEnElHorario: unHorario Hacia: unDestino	llamadas add: (LlamadaInternacional		conDuracion: unaDuracion		conHorario: unHorario		conTarifa: tarifaInternacional		conDestino: unDestino)! !!TeleAlgo methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/18/2022 09:10'!consultarPorLlamadaEfectuadaMasCostosa	| llamadaMasCostosa |	llamadaMasCostosa := llamadas detectMax: [ :llamada | llamada calcularTotalConDescuento ].	^ 'La llamada mas costosa: ', llamadaMasCostosa descripcion! !!TeleAlgo methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/18/2022 20:06'!registrarLlamadaNacionalConDuracion: unaDuracion HechaEnElHorario: unHorario	| tarifa |		tarifa := self calcularTarifaPorMinutoSegunHorario: unHorario.	llamadas add: (LlamadaNacional		conDuracion: unaDuracion		conHorario: unHorario		conTarifa: tarifa)! !!TeleAlgo methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 13:50'!abonoMensual: unAbonoMensual	abonoMensual := unAbonoMensual ! !!TeleAlgo methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/18/2022 09:10'!calcularGastoFinal	^ abonoMensual + (llamadas sum: [ :llamada | llamada calcularTotalConDescuento ]).! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!TeleAlgo class	instanceVariableNames: ''!!TeleAlgo class methodsFor: 'as yet unclassified' stamp: 'CarlosCastillo 4/17/2022 13:51'!conAbonoMensual: unAbonoMensual	^ (self new) abonoMensual: unAbonoMensual! !